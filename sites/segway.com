            var st = window.pageYOffset || document.documentElement.scrollTop; 
                if (st > lastScrollTop){
                    if (window.pageYOffset >= sticky) {
                        navbar.classList.remove("product-sticky-up");
                        navbar.classList.add("product-sticky-down");
                    } else {
                        navbar.classList.remove("product-sticky-up");
                        navbar.classList.remove("product-sticky-down");
                    }
                } else {
                    navbar.classList.remove("product-sticky-down");
                    navbar.classList.add("product-sticky-up");
                    if(jQuery('.qodef-mobile-header:visible').length == 0){ 
                        jQuery(".product-sticky-up").css('top', jQuery(".qodef-page-header").height());
                    }else{
                        jQuery(".product-sticky-up").css('top', jQuery(".qodef-mobile-header-inner").height());
                    }
                }
            lastScrollTop = st <= 0 ? 0 : st;
        }
    }

    jQuery(function() {
        if (jQuery(".sub-nav-tab")[0]){
            myScroll();
        }
        window.onscroll = function() {myScroll()};
    });
})(jQuery)</script></body>
